{
  "name": "prettier-plugin-tailwindcss-extra-plus",
  "version": "0.3.1",
  "description": "Adds Tailwind class sorting to a set of languages that are not yet supported by prettier",
  "license": "MIT",
  "main": "dist/main.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx tsup src/main.ts",
    "test": "npx vitest run",
    "test:watch": "npx vitest watch",
    "lint": "npx tsc --noEmit",
    "fmt": "npx prettier --write .",
    "fmt:check": "npx prettier --check ."
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "peerDependencies": {
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": ">=0.5.13 <1.0.0"
  }
}
